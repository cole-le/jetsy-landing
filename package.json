{
  "name": "jetsy-landing",
  "version": "1.0.0",
  "type": "module",
  "description": "AI-powered startup idea validation landing page with complete user funnel simulation",
  "main": "src/worker.js",
  "scripts": {
    "dev": "vite",
    "dev:worker": "wrangler dev --env development --x-remote-bindings",
    "dev:pages": "npx wrangler pages dev dist --compatibility-date=2024-01-01 --d1 DB=jetsy-leads",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "wrangler deploy",
    "deploy:pages": "npx wrangler pages deploy dist --project-name=jetsy-landing",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "db:create": "wrangler d1 create jetsy-leads",
    "db:migrate": "wrangler d1 execute jetsy-leads --file=./schema.sql",
    "db:query": "wrangler d1 execute jetsy-leads",
    "test": "echo \"No tests specified\" && exit 0",
    "test:image": "node test-image-generation.js",
    "test:option2": "node test-option2-hybrid.js",
    "demo:option2": "node demo-option2.js",
    "test:llm": "node test-llm.js",
    "test:enhanced": "node test-enhanced-landing.js",
    "clear:projects": "node clear-all-projects.js",
    "clear:database": "node clear-database-completely.js",
    "clear:sql": "node clear-database-sql.js",
    "test:buffer": "node test-buffer-fix.js",
    "test:replacement": "node test-image-replacement.js",
    "lint": "echo \"No linter configured\" && exit 0",
    "format": "echo \"No formatter configured\" && exit 0"
  },
  "keywords": [
    "landing-page",
    "startup-validation",
    "saas",
    "cloudflare-workers",
    "cloudflare-d1",
    "lead-generation",
    "funnel-tracking",
    "responsive-design"
  ],
  "author": "Jetsy Team",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.11.0",
    "@supabase/supabase-js": "^2.56.0",
    "dotenv": "^17.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-frame-component": "^5.2.7",
    "react-icons": "^5.5.0",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "vite": "^4.4.0",
    "wrangler": "^4.25.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/jetsy-landing.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/jetsy-landing/issues"
  },
  "homepage": "https://github.com/your-username/jetsy-landing#readme"
}
